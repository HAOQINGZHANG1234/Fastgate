@article{heinrich_ultrafast_2018,
	title = {Ultrafast coherent excitation of a {Ca}\${\textasciicircum}+\$ ion},
	url = {http://arxiv.org/abs/1812.08537},
	abstract = {Trapped ions are a well-studied and promising system for the realization of a scalable quantum computer. Faster quantum gates would greatly improve the applicability of such a system and allow for greater ﬂexibility in the number of calculation steps. In this paper we present a pulsed laser system, delivering picosecond pulses at a repetition rate of 5 GHz and resonant to the 4S1/2 ↔ 4P3/2 transition in 40Ca+ for coherent population transfer to implement fast phase gate operations. The optical pulse train is derived from a mode-locked, stabilized optical frequency comb and inherits its frequency stability. Using a single trapped ion, we implement three diﬀerent techniques for measuring the ion-laser coupling strength and characterizing the pulse train emitted by the laser, and show how all requirements can be met for an implementation of a fast phase gate operation.},
	language = {en},
	urldate = {2019-03-20},
	journal = {arXiv:1812.08537 [physics, physics:quant-ph]},
	author = {Heinrich, D. and Guggemos, M. and Guevara-Bertsch, M. and Hussain, M. I. and Roos, C. F. and Blatt, R.},
	month = dec,
	year = {2018},
	note = {arXiv: 1812.08537},
	keywords = {Quantum Physics, Physics - Atomic Physics},
	file = {Heinrich 等。 - 2018 - Ultrafast coherent excitation of a Ca\$^+\$ ion.pdf:files/53/Heinrich 等。 - 2018 - Ultrafast coherent excitation of a Ca\$^+\$ ion.pdf:application/pdf}
}

@article{ratcliffe_micromotion-enhanced_2019,
	title = {Micromotion-{Enhanced} {Fast} {Entangling} {Gates} {For} {Trapped} {Ion} {Quantum} {Computing}},
	url = {http://arxiv.org/abs/1902.06364},
	abstract = {RF-induced micromotion in trapped ion systems is typically minimised or circumvented to avoid off-resonant couplings for adiabatic processes such as multi-ion gate operations. Non-adiabatic entangling gates (so-called `fast gates') do not require resolution of specific motional sidebands, but we find that gates designed for micromotion-free environments have significantly reduced fidelity in the presence of micromotion. We show that when fast gates are designed with the RF-induced micromotion in mind, they can in fact out-perform fast gates in the absence of micromotion. This enhancement is present for all trapping parameters and is robust to realistic sources of experimental error. This result paves the way for fast two-qubit entangling gates on scalable 2D architectures, where micromotion is necessarily present on at least one inter-ion axis.},
	language = {en},
	urldate = {2019-03-20},
	journal = {arXiv:1902.06364 [quant-ph]},
	author = {Ratcliffe, Alexander K. and Oberg, Lachlan M. and Hope, Joseph J.},
	month = feb,
	year = {2019},
	note = {arXiv: 1902.06364},
	keywords = {Quantum Physics},
	file = {Ratcliffe 等。 - 2019 - Micromotion-Enhanced Fast Entangling Gates For Tra.pdf:files/55/Ratcliffe 等。 - 2019 - Micromotion-Enhanced Fast Entangling Gates For Tra.pdf:application/pdf}
}

@article{bentley_trapped_2015,
	title = {Trapped ion scaling with pulsed fast gates},
	volume = {17},
	issn = {1367-2630},
	url = {http://arxiv.org/abs/1507.02783},
	doi = {10.1088/1367-2630/17/10/103025},
	abstract = {Fast entangling gates for trapped ions offer vastly improved gate operation times relative to implemented gates, as well as approaches to trap scaling. Gates on neighbouring ions only involve local ions when performed sufficiently fast, and we find that even a fast gate between distant ions with few degrees of freedom restores all the motional modes given more stringent gate speed conditions. We compare pulsed fast gate schemes, defined by a timescale faster than the trap period, and find that our proposed scheme has less stringent requirements on laser repetition rate for achieving arbitrary gate time targets and infidelities well below \$10{\textasciicircum}\{-4\}\$. By extending gate schemes to ion crystals, we explore the effect of ion number on gate fidelity for coupling neighbouring pairs of ions in large crystals. Inter-ion distance determines the gate time, and a factor of five increase in repetition rate, or correspondingly the laser power, reduces the infidelity by almost two orders of magnitude. We also apply our fast gate scheme to entangle the first and last ions in a crystal. As the number of ions in the crystal increases, significant increases in the laser power are required to provide the short gate times corresponding to fidelity above 0.99.},
	language = {en},
	number = {10},
	urldate = {2019-03-20},
	journal = {New Journal of Physics},
	author = {Bentley, C. D. B. and Carvalho, A. R. R. and Hope, J. J.},
	month = oct,
	year = {2015},
	note = {arXiv: 1507.02783},
	keywords = {Quantum Physics},
	pages = {103025},
	file = {Bentley 等。 - 2015 - Trapped ion scaling with pulsed fast gates.pdf:files/57/Bentley 等。 - 2015 - Trapped ion scaling with pulsed fast gates.pdf:application/pdf}
}

@article{hussain_ultrafast_2016,
	title = {Ultrafast, high repetition rate, ultraviolet, fiber-laser-based source: application towards {Yb}$^{\textrm{\&\#x0002B;}}$ fast quantum-logic},
	volume = {24},
	copyright = {\&\#169; 2016 Optical Society of America},
	issn = {1094-4087},
	shorttitle = {Ultrafast, high repetition rate, ultraviolet, fiber-laser-based source},
	url = {https://www.osapublishing.org/oe/abstract.cfm?uri=oe-24-15-16638},
	doi = {10.1364/OE.24.016638},
	abstract = {Trapped ions are one of the most promising approaches for the realization of a universal quantum computer. Faster quantum logic gates could dramatically improve the performance of trapped-ion quantum computers, and require the development of suitable high repetition rate pulsed lasers. Here we report on a robust frequency upconverted fiber laser based source, able to deliver 2.5 ps ultraviolet (UV) pulses at a stabilized repetition rate of 300.00000 MHz with an average power of 190 mW. The laser wavelength is resonant with the strong transition in Ytterbium (Yb\&\#x0002B;) at 369.53 nm and its repetition rate can be scaled up using high harmonic mode locking. We show that our source can produce arbitrary pulse patterns using a programmable pulse pattern generator and fast modulating components. Finally, simulations demonstrate that our laser is capable of performing resonant, temperature-insensitive, two-qubit quantum logic gates on trapped Yb\&\#x0002B; ions faster than the trap period and with fidelity above 99\&\#x00025;.},
	language = {EN},
	number = {15},
	urldate = {2019-03-20},
	journal = {Opt. Express, OE},
	author = {Hussain, Mahmood Irtiza and Petrasiunas, Matthew Joseph and Bentley, Christopher D. B. and Taylor, Richard L. and Carvalho, André R. R. and Hope, Joseph J. and Streed, Erik W. and Lobino, Mirko and Kielpinski, David},
	month = jul,
	year = {2016},
	keywords = {Amplified spontaneous emission, Fiber lasers, Infrared lasers, Pulsed fiber lasers, Sum frequency generation, Tunable diode lasers},
	pages = {16638--16648},
	file = {全文:files/75/Hussain 等。 - 2016 - Ultrafast, high repetition rate, ultraviolet, fibe.pdf:application/pdf;Snapshot:files/74/abstract.html:text/html}
}

