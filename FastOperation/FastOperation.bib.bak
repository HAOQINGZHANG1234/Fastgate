@article{zhu_arbitrary-speed_2006,
	title = {Arbitrary-speed quantum gates within large ion crystals through minimum control of laser beams},
	volume = {73},
	issn = {0295-5075, 1286-4854},
	url = {http://stacks.iop.org/0295-5075/73/i=4/a=485?key=crossref.93e3138ffab2e9c547e4af5dc22f1e06},
	doi = {10.1209/epl/i2005-10424-4},
	abstract = {We propose a scheme to implement arbitrary-speed quantum entangling gates on two trapped ions immersed in a large linear crystal of ions, with minimal control of laser beams. For gate speeds slower than the oscillation frequencies in the trap, a single appropriately detuned laser pulse is suﬃcient for high-ﬁdelity gates. For gate speeds comparable to or faster than the local ion oscillation frequency, we discover a ﬁve-pulse protocol that exploits only the local phonon modes. This points to a method for eﬃciently scaling the ion trap quantum computer without shuttling ions.},
	language = {en},
	number = {4},
	urldate = {2019-03-20},
	journal = {Europhysics Letters (EPL)},
	author = {Zhu, Shi-Liang and Monroe, C and Duan, L.-M},
	month = feb,
	year = {2006},
	pages = {485--491},
	file = {Zhu 等。 - 2006 - Arbitrary-speed quantum gates within large ion cry.pdf:files/10/Zhu 等。 - 2006 - Arbitrary-speed quantum gates within large ion cry.pdf:application/pdf}
}

@article{ho_ultrafast_2018,
	title = {Ultrafast {State} {Preparation} via the {Quantum} {Approximate} {Optimization} {Algorithm} with {Long} {Range} {Interactions}},
	url = {http://arxiv.org/abs/1810.04817},
	abstract = {State preparation protocols ideally require as minimal a circuit depth as possible, in order to be implemented in near-term quantum devices. Motivated by long range interactions (LRI) intrinsic to many experimental platforms (trapped ions, Rydberg atom arrays, etc.), we investigate the performance of the quantum approximate optimization algorithm (QAOA) with LRIs for the preparation of non-trivial quantum states. We show that this approach leads to extremely efficient state preparation: for example, Greene-Horne-Zeilinger (GHZ) states can be prepared with O(1) circuit depth, and a quantum critical point of the long range transverse field Ising model (TFIM) can be prepared with {\textgreater} 99\% fidelity on a 100 qubit system with only one iteration of QAOA. Furthermore, we show that QAOA with LRIs is a promising route for exploring generic points in the phase diagram of the long-range TFIM. Our approach thus provides concrete, ultrafast protocols for quantum simulators equipped with long range interactions.},
	language = {en},
	urldate = {2019-03-20},
	journal = {arXiv:1810.04817 [cond-mat, physics:quant-ph]},
	author = {Ho, Wen Wei and Jonay, Cheryne and Hsieh, Timothy H.},
	month = oct,
	year = {2018},
	note = {arXiv: 1810.04817},
	keywords = {Condensed Matter - Strongly Correlated Electrons, Quantum Physics},
	file = {Ho 等。 - 2018 - Ultrafast State Preparation via the Quantum Approx.pdf:files/11/Ho 等。 - 2018 - Ultrafast State Preparation via the Quantum Approx.pdf:application/pdf}
}

